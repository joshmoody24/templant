{
  "name": "templant",
  "version": "0.0.5",
  "description": "Translate between different template languages",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joshmoody24/templant.git"
  },
  "bugs": {
    "url": "https://github.com/joshmoody24/templant/issues"
  },
  "homepage": "https://github.com/joshmoody24/templant#readme",
  "keywords": [
    "template",
    "nunjucks",
    "mustache",
    "handlebars",
    "ejs",
    "liquid",
    "pug",
    "translation"
  ],
  "type": "module",
  "files": [
    "dist/"
  ],
  "main": "dist/translate.js",
  "types": "dist/translate.d.ts",
  "exports": {
    ".": "./dist/translate.js"
  },
  "scripts": {
    "build": "mkdir -p dist && cp src/translate.js dist/translate.js && cp src/translate.d.ts dist/translate.d.ts",
    "publish": "npm run build && npm publish --access public",
    "test": "node --test",
    "lint": "eslint .",
    "format": "prettier --write ."
  },
  "dependencies": {
    "liquidjs": "^10.21.1",
    "nunjucks": "^3.2.4"
  },
  "devDependencies": {
    "eslint": "^9.34.0",
    "prettier": "^3.6.2"
  }
}
