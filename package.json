{
  "name": "@joshmoody24/transplate",
  "version": "0.0.1",
  "description": "Translate between different template languages",
  "keywords": [
    "template",
    "nunjucks",
    "mustache",
    "handlebars",
    "ejs",
    "liquid",
    "pug",
    "translation"
  ],
  "type": "module",
  "files": [
    "dist/"
  ],
  "main": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.cjs"
    }
  },
  "devDependencies": {
    "shadow-cljs": "3.2.0"
  },
  "scripts": {
    "build": "shadow-cljs release lib-cjs && shadow-cljs release lib-esm"
  }
}
